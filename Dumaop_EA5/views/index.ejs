<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>JYP Hospital | Patient Database</title>
    <link rel="stylesheet" href="/css/style.css">
</head>
<body>
    <div class="container">
        <h1>JYP Hospital | Patient Database</h1>
        <table>
            <thead>
                <tr>
                    <th>Patient ID</th>
                    <th>Last Name</th>
                    <th>First Name</th>
                    <th>Age</th>
                </tr>
            </thead>
            <tbody>
                <% patients.forEach(patient => { %>
                    <tr>
                        <td><%= patient.patientId %></td>
                        <td><%= patient.lastName %></td>
                        <td><%= patient.firstName %></td>
                        <td><%= patient.age %></td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</body>
</html>
